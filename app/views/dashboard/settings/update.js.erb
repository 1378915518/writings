<% if @space.errors.empty? %>
  AlertMessage.show({
    type: 'success',
    text: '<%= t('success') %>',
    timeout: 1500
  });
  $("#user-link").attr("href", "http://" + '<%= @space.host %>');
<% else %>
  AlertMessage.show({
    type: 'error',
    text: '<%= j current_user.errors.full_messages.join %>'
  });
<% end %>
